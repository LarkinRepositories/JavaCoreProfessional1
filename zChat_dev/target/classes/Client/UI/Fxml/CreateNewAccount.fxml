<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<AnchorPane prefHeight="394.0" prefWidth="566.0" stylesheets="@../css/CreateNewAccount.css" xmlns="http://javafx.com/javafx/8.0.221" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.UI.Controller.CreateNewAccount">
   <children>
      <AnchorPane id="accountCreationPane" fx:id="accountCreationPane" prefHeight="394.0" prefWidth="576.0">
         <children>
            <TextField fx:id="loginField" layoutX="166.0" layoutY="132.0" prefHeight="25.0" prefWidth="234.0" promptText="login" />
            <TextField fx:id="nicknameField" layoutX="166.0" layoutY="183.0" prefHeight="25.0" prefWidth="234.0" promptText="nickname" />
            <PasswordField fx:id="passwordField" layoutX="166.0" layoutY="236.0" prefHeight="25.0" prefWidth="234.0" promptText="password" />
            <Text fill="WHITE" layoutX="169.0" layoutY="122.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login" />
            <Text fill="WHITE" layoutX="167.0" layoutY="231.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
            <Text fill="WHITE" layoutX="167.0" layoutY="179.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nickname" />
            <Button id="createBtn" fx:id="createBtn" layoutX="348.0" layoutY="290.0" mnemonicParsing="false" text="Create" onAction="#createNew"/>
            <Text id="alreadyHave" fx:id="alreadyHave" fill="#0112f8" layoutX="166.0" layoutY="279.0" strokeType="OUTSIDE" strokeWidth="0.0" text="I already have an account" onMouseClicked="#alreadyHave" />
            <Text fill="WHITE" layoutX="176.0" layoutY="71.0" strokeType="OUTSIDE" strokeWidth="0.0" text="zChat Account creation ">
               <font>
                  <Font size="20.0" />
               </font>
            </Text>
            <Button layoutX="496.0" layoutY="14.0" mnemonicParsing="false" text="Exit"></Button>
         </children></AnchorPane>
   </children>
</AnchorPane>
